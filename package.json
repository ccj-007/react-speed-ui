{
  "name": "react-speed-ui",
  "version": "0.1.4",
  "author": {
    "name": "Chase Chen",
    "email": "596487930@qq.com",
    "url": "https://github.com/ccj-007/speed-ui"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "tpl": "node src/generator.js",
    "clear-storybook": "rimraf node_modules/.cache/storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6012 -s public ",
    "lib": "rollup --config rollup.config.build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf types lib esm dist",
    "rimraf": "rimraf ./node_modules",
    "gulp": "npm run clean && gulp",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.2",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.5",
    "axios": "^0.27.2",
    "babel-plugin-macros": "^3.1.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "ejs": "^3.1.8",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.2",
    "sass-loader": "^13.0.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.11.9",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.1",
    "customize-cra": "^1.0.0",
    "gh-pages": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "through2": "^4.0.2",
    "webpack": "^5.74.0"
  },
  "types": "dist/types/index.js",
  "main": "dist/lib/components/index.js",
  "module": "dist/esm/components/index.js",
  "private": false,
  "homepage": "http://ccj-007.github.io/react-speed-ui",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/**"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off",
          "react-hooks/rules-of-hooks": "off",
          "@typescript-eslint/no-unused-vars": "off",
          "react-hooks/exhaustive-deps": "off",
          "no-script-url": "off",
          "array-callback-return": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "*.scss"
  ]
}
